<%= form_with model: @kitten, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :age %>
    <%= f.number_field :age %>

    <%= f.label :softness %>
    <%= f.number_field :softness, in: 1.0..10.0 %>

    <%= f.label :cuteness %>
    <%= f.number_field :cuteness, in: 1.0..10.0 %>
    
    <%= f.submit %>
<% end %>